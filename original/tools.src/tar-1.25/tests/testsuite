echo Short Listing
tar tf \$TEST_DATA_DIR/abc.tar
echo Verbose Listing
tar --utc -tvf \$TEST_DATA_DIR/abc.tar
echo Extracted directory
tar xf \$TEST_DATA_DIR/abc.tar
find abc|sort
)"
