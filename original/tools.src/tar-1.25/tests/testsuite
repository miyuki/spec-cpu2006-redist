echo \"Test archive\"
tar --record-size=512 -t -M -f arc.1 -f arc.2
echo \"Compare archive\"
tar --record-size=512 -d -M -f arc.1 -f arc.2
)"
echo sparsemv.at:31 >"$at_check_line_file"

